{
  "manifest_version": 3,
  "name": "Tech Stack Detector Pro",
  "version": "1.0.0",
  "description": "Professional tech stack analysis. Detect frameworks, backend, hosting, and CDNs instantly.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.svg",
      "32": "icon.svg",
      "48": "icon.svg",
      "128": "icon.svg"
    }
  },
  "icons": {
    "16": "icon.svg",
    "32": "icon.svg",
    "48": "icon.svg",
    "128": "icon.svg"
  }
}