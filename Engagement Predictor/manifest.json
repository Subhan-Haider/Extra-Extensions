{
  "manifest_version": 3,
  "name": "Engagement Predictor",
  "version": "1.0.0",
  "description": "Predict the engagement level of your post before publishing. Get suggestions for improvement.",
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.svg",
      "32": "icon.svg",
      "48": "icon.svg",
      "128": "icon.svg"
    }
  },
  "icons": {
    "16": "icon.svg",
    "32": "icon.svg",
    "48": "icon.svg",
    "128": "icon.svg"
  }
}