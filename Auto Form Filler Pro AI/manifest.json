{
  "manifest_version": 3,
  "name": "Auto Form Filler Pro AI",
  "version": "1.0.0",
  "description": "Smart AI-powered form filling. Understands context and drafts personalized answers instantly.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "filler.css"
      ]
    }
  ],
  "icons": {
    "16": "icon.svg",
    "32": "icon.svg",
    "48": "icon.svg",
    "128": "icon.svg"
  },
  "action": {}
}